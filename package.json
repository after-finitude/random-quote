{
  "name": "bookcase",
  "version": "0.0.1",
  "description": "Bookcase",
  "author": "Alexander Zenov",
  "private": true,
  "scripts": {
    "dev": "rollup -c -w",
    "start": "sirv public",
    "build": "pnpm run validate && rimraf public/build && rollup -c",
    "clean": "rimraf public/build node_modules",
    "validate": "svelte-check"
  },
  "size-limit": [
    {
      "path": "public"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "11.1.1",
    "@tsconfig/svelte": "1.0.10",
    "rimraf": "3.0.2",
    "rollup": "2.38.5",
    "rollup-plugin-filesize": "9.1.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "sass": "^1.32.6",
    "svelte": "3.32.1",
    "svelte-check": "1.1.32",
    "svelte-preprocess": "4.6.6",
    "svelte-select": "^3.16.1",
    "tslib": "2.1.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "sirv-cli": "1.0.11"
  }
}
